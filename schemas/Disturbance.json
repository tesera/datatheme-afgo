{
  "name": "Disturbance",
  "path": "Disturbance.csv",
  "schema": {
    "fields": [
      {
        "name": "CompanyPlotNumber",
        "type": "string",
        "constraints": {
          "required": true
        }
      },
      {
        "name": "Company",
        "type": "string",
        "constraints": {
          "required": true,
          "pattern": "/^(AINS|GOA|APLY|ALPC|ANC|BLUE|CFPL|CFS|DAIS|FOFP|BUCH|MDFP|MWWC|SLPC|SPRA|SUND|SFPI|HLFP|TOLK|TOSL|UOA|VAND|WFML|WEYR|WEYR|UNKN|HPFP)$/"
        }
      },
      {
        "name": "DisturbanceNumber",
        "type": "number",
        "constraints": {
          "min": 12
        }
      },
      {
        "name": "DisturbanceCode",
        "type": "string",
        "constraints": {
          "pattern": "/^(BU|DF|MI|MU|NDD|NDW|DC|HL|MLU|NDC|NDI|FI|DA)$/"
        }
      },
      {
        "name": "DisturbanceYear",
        "type": "number",
        "constraints": {
          "min": 1800,
          "max": 2050
        }
      },
      {
        "name": "DisturbanceMonth",
        "type": "number",
        "constraints": {
          "min": 1,
          "max": 12
        }
      },
      {
        "name": "DisturbanceDay",
        "type": "number",
        "constraints": {
          "min": 1,
          "max": 31
        }
      },
      {
        "name": "DisturbanceComment",
        "type": "string"
      }
    ]
  },
  "primaryKey": ["CompanyPlotNumber", "Company"],
  "foreignKeys": [
    {
      "fields": ["CompanyPlotNumber", "Company"],
      "reference": {
        "resource": "Plot",
        "fields": ["CompanyPlotNumber", "Company"]
      }
    }
  ]
}