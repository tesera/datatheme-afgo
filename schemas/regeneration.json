{
  "name": "regeneration",
  "path": "regeneration.csv",
  "schema": {
    "fields": [
      {
        "name": "company",
        "type": "string",
        "raises": "error",
        "constraints": {
          "required": true,
          "pattern": "/^(AINS|GOA|APLY|ALPC|ANC|BLUE|CFPL|CFS|DAIS|FOFP|MDFP|MWWC|SLPC|SPRA|SUND|SFPI|HLFP|TOLK|TOSL|UOA|VAND|WFML|WYGP|WYPM|UNKN|HPFP)$/"
        }
      },
      {
        "name": "company_plot_number",
        "type": "string",
        "raises": "error",
        "constraints": {
          "required": true,
          "maxLength": 15
        }
      },
      {
        "name": "measurement_number",
        "type": "integer",
        "raises": "error",
        "constraints": {
          "required": true,
          "maxLength": 2
        }
      },
      {
        "name": "regeneration_plot_name",
        "type": "string",
        "raises": "error",
        "constraints": {
          "required": true,
          "pattern": "/^(R1|R2|R3|R4|R5|R6|R7|R8|R9|R10)$/"
        }
      },
      {
        "name": "species",
        "type": "string",
        "raises": "error",
        "constraints": {
          "required": true,
          "pattern": "/^(A|Aw|Pb|Bw|Ax|P|Pl|Pw|Pa|Pf|Pj|Px|Sw|Se|Sb|Sx|Fb|Fa|Fd|Lt|Lw|La|Ls|Ms|No)$/"
        }
      },
      {
        "name": "regeneration_count",
        "type": "integer",
        "constraints": {
          "minimum": 0,
          "maximum": 9999
        }
      },
      {
        "name": "regeneration_comment",
        "type": "string",
        "constraints":{
          "maxLength": 250
        }
      }
    ]
  },
  "primaryKey": [
    "company",
    "company_plot_number",
    "measurement_number",
    "regeneration_plot_name",
    "species"
  ],
  "foreignKeys": [
    {
      "fields": [
        "company",
        "company_plot_number",
        "measurement_number"
      ],
      "reference": {
        "resource": "plot_measurement",
        "fields": [
          "company",
          "company_plot_number",
          "measurement_number"
        ]
      }
    }
  ]
}
