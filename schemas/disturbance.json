{
  "name": "disturbance",
  "path": "disturbance.csv",
  "schema": {
    "fields": [
      {
        "name": "company",
        "type": "string",
        "constraints": {
          "required": true,
          "pattern": "/^(AINS|GOA|APLY|ALPC|ANC|BLUE|CFPL|CFS|DAIS|FOFP|BUCH|MDFP|MWWC|SLPC|SPRA|SUND|SFPI|HLFP|TOLK|TOSL|UOA|VAND|WFML|WYGP|WYPM|UNKN|HPFP)$/"
        }
      },
      {
        "name": "company_plot_number",
        "type": "string",
        "constraints": {
          "required": true
        }
      },
      {
        "name": "disturbance_number",
        "type": "integer",
        "constraints": {
          "min": 1,
          "max": 12
        }
      },
      {
        "name": "disturbance_code",
        "type": "string",
        "constraints": {
          "pattern": "/^(BU|DF|MI|MU|NDD|NDW|DC|HL|MLU|NDC|NDI|FI|DA)$/"
        }
      },
      {
        "name": "disturbance_year",
        "type": "integer",
        "constraints": {
          "min": 1900,
          "max": 2050
        }
      },
      {
        "name": "disturbance_month",
        "type": "integer",
        "constraints": {
          "min": 1,
          "max": 12
        }
      },
      {
        "name": "disturbance_day",
        "type": "integer",
        "constraints": {
          "min": 1,
          "max": 31
        }
      },
      {
        "name": "disturbance_comment",
        "type": "string"
      }
    ]
  },
  "primaryKey": ["company", "company_plot_number","disturbance_number"],
  "foreignKeys": [
    {
      "fields": ["company_plot_number", "company"],
      "reference": {
        "resource": "Plot",
        "fields": ["company_plot_number", "company"]
      }
    }
  ]
}