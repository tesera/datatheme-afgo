{
  "name": "Treatment",
  "path": "Treatment.csv",
  "schema": {
    "fields": [
      {
        "name": "CompanyPlotNumber",
        "type": "string",
        "constraints": {
          "required": true
        }
      },
      {
        "name": "Company",
        "type": "string",
        "constraints": {
          "required": true,
          "pattern": "/^(AINS|GOA|APLY|ALPC|ANC|BLUE|CFPL|CFS|DAIS|FOFP|BUCH|MDFP|MWWC|SLPC|SPRA|SUND|SFPI|HLFP|TOLK|TOSL|UOA|VAND|WFML|WEYR|WEYR|UNKN|HPFP)$/"
        }
      },
      {
        "name": "TreatmentNumber",
        "type": "number",
        "constraints": {
          "min": 1,
          "max": 12
        }
      },
      {
        "name": "TreatmentCode",
        "type": "string",
        "constraints": {
          "pattern": "/^(NT|P|H|M|C|B|TH|TW|F|PC|CT|S|UP|UA|D|NK)$/"
        }
      },
      {
        "name": "TreatmentYear",
        "type": "number",
        "constraints": {
          "min": 1800,
          "max": 2050
        }
      },
      {
        "name": "TreatmentMonth",
        "type": "number",
        "constraints": {
          "min": 1,
          "max": 12
        }
      },
      {
        "name": "TreatmentDay",
        "type": "number",
        "constraints": {
          "min": 1,
          "max": 31
        }
      },
      {
        "name": "TreatmentComment",
        "type": "string"
      }
    ]
  },
  "primaryKey": ["CompanyPlotNumber", "Company"],
  "foreignKeys": [
    {
      "fields": ["CompanyPlotNumber", "Company"],
      "reference": {
        "resource": "Plot",
        "fields": ["CompanyPlotNumber", "Company"]
      }
    }
  ]
}