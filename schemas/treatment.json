{
  "name": "treatment",
  "path": "treatment.csv",
  "schema": {
    "fields": [
      {
        "name": "company",
        "type": "string",
        "constraints": {
          "required": true,
          "pattern": "/^(AINS|GOA|APLY|ALPC|ANC|BLUE|CFPL|CFS|DAIS|FOFP|BUCH|MDFP|MWWC|SLPC|SPRA|SUND|SFPI|HLFP|TOLK|TOSL|UOA|VAND|WFML|WYGP|WYPM|UNKN|HPFP)$/"
        }
      },
      {
        "name": "company_plot_number",
        "type": "string",
        "constraints": {
          "required": true
        }
      },
      {
        "name": "treatment_number",
        "type": "integer",
        "constraints": {
          "min": 1,
          "max": 12
        }
      },
      {
        "name": "treatment_code",
        "type": "string",
        "constraints": {
          "pattern": "/^(NT|P|H|M|C|B|TH|TW|F|FI|PC|CT|S|UP|UA|D|NK)$/"
        }
      },
      {
        "name": "treatment_year",
        "type": "integer",
        "constraints": {
          "min": 1900,
          "max": 2050
        }
      },
      {
        "name": "treatment_month",
        "type": "integer",
        "constraints": {
          "min": 1,
          "max": 12
        }
      },
      {
        "name": "treatment_day",
        "type": "integer",
        "constraints": {
          "min": 1,
          "max": 31
        }
      },
      {
        "name": "treatment_comment",
        "type": "string"
      }
    ]
  },
  "primaryKey": ["company", "company_plot_number", "treatment_number"],
  "foreignKeys": [
    {
      "fields": ["company_plot_number", "company"],
      "reference": {
        "resource": "plot",
        "fields": ["company_plot_number", "company"]
      }
    }
  ]
}