{
  "name": "regeneration",
  "path": "regeneration.csv",
  "schema": {
    "fields": [
      {
        "name": "company",
        "type": "string",
        "constraints": {
          "required": true,
          "pattern": "/^(AINS|GOA|APLY|ALPC|ANC|BLUE|CFPL|CFS|DAIS|FOFP|BUCH|MDFP|MWWC|SLPC|SPRA|SUND|SFPI|HLFP|TOLK|TOSL|UOA|VAND|WFML|WYGP|WYPM|UNKN|HPFP)$/"
        }
      },
      {
        "name": "company_plot_number",
        "type": "string",
        "constraints": {
          "required": true
        }
      },
      {
        "name": "measurement_number",
        "type": "integer",
        "constraints": {
          "required": true
        }
      },
      {
        "name": "regeneration_plot_name",
        "type": "string",
        "constraints": {
          "pattern": "/^(R1|R2|R3|R4|R5|R6|R7|R8|R9|R10)$/",
          "required": true
        }
      },
      {
        "name": "species",
        "type": "string",
        "constraints": {
          "pattern": "/^(Aw|Pb|Bw|Ax|Dd|Dc|Pl|Pw|Pa|Pf|Pj|Px|Sw|Se|Sb|Sx|Fb|Fa|Fd|Lt|Lw|La|Ls|Du|Ms|No)$/",
          "required": true
        }
      },
      {
        "name": "regeneration_count",
        "type": "number",
        "constraints": {
          "min": 0,
          "max": 9999
        }
      },
      {
        "name": "regeneration_comment",
        "type": "string"
      }
    ]
  },
  "primaryKey": ["comnpany", "company_plot_number","measurement_number", "regeneration_plot_name", "species"],
  "foreignKeys": [
    {
      "fields": ["company", "company_plot_number"],
      "reference": {
        "resource": "plot",
        "fields": ["company", "company_plot_number"]
      }
    }
  ]
}