{
  "name": "plot",
  "path": "plot.csv",
  "schema": {
    "fields": [
      {
        "name": "company",
        "type": "string",
        "raises": "error",
        "constraints": {
          "required": true,
          "pattern": "/^(AINS|GOA|APLY|ALPC|ANC|BLUE|CFPL|CFS|DAIS|FOFP|MDFP|MWWC|SLPC|SPRA|SUND|SFPI|HLFP|TOLK|TOSL|UOA|VAND|WFML|WYGP|WYPM|UNKN|HPFP)$/"
        }
      },
      {
        "name": "company_plot_number",
        "type": "string",
        "raises": "error",
        "constraints": {
          "required": true,
          "unique": true,
          "maxLength": 15
        }
      },
      {
        "name": "company_stand_number",
        "type": "string",
        "constraints": {
          "maxLength": 15
        }
      },
      {
        "name": "establishment_year",
        "type": "integer",
        "raises": "error",
        "constraints": {
          "required": true,
          "minimum": 1900,
          "maximum": 2050
        }
      },
      {
        "name": "establishment_month",
        "type": "integer",
        "raises": "warning",
        "constraints": {
          "required": true,
          "minimum": 1,
          "maximum": 12
        }
      },
      {
        "name": "establishment_day",
        "type": "integer",
        "raises": "warning",
        "constraints": {
          "required": true,
          "minimum": 1,
          "maximum": 31
        }
      },
      {
        "name": "fmu",
        "type": "string",
        "raises": "warning",
        "constraints": {
          "pattern": "/^(A10|A11|A12|A13|A14|A15|A4|A5|A6|A7|A8|A9|B10|B11|B9|BO1|BO2|C4|C5|CO1|CO2|E1|E10|E11|E13|E14|E15|E2|E3|E4|E5|E6|E7|E8|E9|EO1|F1|F10|F11|F14|F20|F21|F23|F24|F25|FO1|G1|G10|G11|G12|G13|G14|G15|G16|G2|G3|G4|G5|G6|G7|G8|G9|GO1|GO2|GO3|GO4|H1|L1|L11|L2|L3|L8|L9|LO1|M1|M10|M3|M4|M7|M9|NA|P1|P10|P11|P13|P14|P19|P20|P21|P3|P4|P5|P6|P7|P8|P9|PO1|PO2|PO3|R1|R10|R11|R12|R13|R14|R2|R2U|R3|R4|R4Y|RO1|S10|S11|S14|S15|S16|S17|S18|S19|S20|S21|S22|S23|S24|S25|S7|S9|SO1|SO2|W1|W10|W11|W13|W14|W15|W2|W3|W4|W5|W6|W8|WO1|WO2|WO3|E14)$/"
        }
      },
      {
        "name": "fma",
        "type": "string",
        "raises": "warning",
        "constraints": {
          "maxLength": 30
        }
      },
      {
        "name": "ats_township",
        "type": "integer",
        "raises": "warning",
        "constraints": {
          "required": true,
          "minimum": 1,
          "maximum": 126
        }
      },
      {
        "name": "ats_range",
        "type": "integer",
        "raises": "warning",
        "constraints": {
          "required": true,
          "minimum": 1,
          "maximum": 26
        }
      },
      {
        "name": "ats_meridian",
        "type": "integer",
        "raises": "warning",
        "constraints": {
          "required": true,
          "minimum": 4,
          "maximum": 6
        }
      },
      {
        "name": "ats_section",
        "type": "integer",
        "raises": "warning",
        "constraints": {
          "required": true,
          "minimum": 1,
          "maximum": 36
        }
      },
      {
        "name": "opening_number",
        "type": "string",
        "constraints": {
          "maxLength": 11
        }
      },
      {
        "name": "sampling_unit_number",
        "type": "string",
        "constraints": {
          "maxLength": 3
        }
      },
      {
        "name": "topographic_position",
        "type": "integer",
        "raises": "warning",
        "constraints": {
          "pattern": "/^(1|2|3|4|5|6|7)$/"
        }
      },
      {
        "name": "elevation",
        "type": "integer",
        "raises": "warning",
        "constraints": {
          "minimum": 0,
          "maximum": 3747
        }
      },
      {
        "name": "slope",
        "type": "integer",
        "raises": "warning",
        "constraints": {
          "minimum": 0,
          "maximum": 200
        }
      },
      {
        "name": "aspect",
        "type": "string",
        "raises": "warning",
        "constraints": {
          "pattern": "/^(N|E|S|W|NE|SE|SW|NW|NA)$/"
        }
      },
      {
        "name": "x_coord",
        "type": "number",
        "raises": "warning",
        "constraints": {
          "minimum": -1030400,
          "maximum": -111700
        }
      },
      {
        "name": "y_coord",
        "type": "number",
        "raises": "warning",
        "constraints": {
          "minimum": 5643600,
          "maximum": 6702500
        }
      },
      {
        "name": "utm_zone",
        "type": "string",
        "raises": "warning",
        "constraints": {
          "required": true,
          "pattern": "/^(UTM11|UTM12)$/"
        }
      },
      {
        "name": "datum",
        "type": "string",
        "raises": "warning",
        "constraints": {
          "required": true,
          "pattern": "/^(NAD27|NAD83)$/"
        }
      },
      {
        "name": "latitude",
        "type": "number",
        "constraints": {
          "minimum": 49.00000,
          "maximum": 60.00000
        }
      },
      {
        "name": "longitude",
        "type": "number",
        "constraints": {
          "minimum": -120.00000,
          "maximum": -110.00000
        }
      },
      {
        "name": "natural_subregion",
        "type": "string",
        "raises": "error",
        "constraints": {
          "required": true,
          "pattern": "/^(CM|DM|NM|BSA|PAD|LBH|UBH|AP|ALP|SA|MT|UF|LF|KU|FP|PRP|CP|DMG|FF|NF|MG)$/"
        }
      },
      {
        "name": "ecosite_guide",
        "type": "string",
        "raises": "warning",
        "constraints": {
          "required": true,
          "pattern": "/^(N|WC|SW)$/"
        }
      },
      {
        "name": "ecosite",
        "type": "string",
        "raises": "warning",        
        "constraints": {
          "required": true,
          "pattern": "/^(a|b|c|d|e|f|g|h|i|j|k|l|m|n)$/"
        }
      },
      {
        "name": "ecosite_phase",
        "type": "string",
        "raises": "warning",
        "constraints": {
          "required": true,
          "pattern": "/^(a1|b1|b2|b3|b4|c1|d1|d2|d3|e1|e2|e3|f1|f2|f3|g1|h1|i1|i2|j1|j2|k1|k2|k3|l1|h2|j3|a2|a3|g2|h3|b5|c2|c3|c4|c5|d4|e4|f4|f5|f6|g3|i3|l2|l3|m1|m2|m3|n1)$/"
        }
      },
      {
        "name": "plot_comment",
        "type": "string",
        "constraints": {
          "maxLength": 250
        }
      }
    ]
  },
  "primaryKey": [
    "company",
    "company_plot_number"
  ]
}