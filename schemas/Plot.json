{
  "name": "Plot",
  "path": "Plot.csv",
  "schema": {
    "fields": [
      {
        "name": "CompanyPlotNumber",
        "type": "string",
        "constraints": {
          "required": true,
          "unique": true
        }
      },
      {
        "name": "Company",
        "type": "string",
        "constraints": {
          "required": true,
          "pattern": "/^(AINS|GOA|APLY|ALPC|ANC|BLUE|CFPL|CFS|DAIS|FOFP|BUCH|MDFP|MWWC|SLPC|SPRA|SUND|SFPI|HLFP|TOLK|TOSL|UOA|VAND|WFML|WEYR|WEYR|UNKN|HPFP)$/"
        }
      },
      {
        "name": "CompanyStandNumber",
        "type": "string"
      },
      {
        "name": "EstablishmentYear",
        "type": "number",
        "constraints": {
          "min": 1800,
          "max": 2050
        }
      },
      {
        "name": "EstablishmentMonth",
        "type": "number",
        "constraints": {
          "min": 1,
          "max": 12
        }
      },
      {
        "name": "EstablishmentDay",
        "type": "number",
        "constraints": {
          "min": 1,
          "max": 31
        }
      },
      {
        "name": "FMU",
        "type": "string",
        "constraints": {
          "pattern": "/^(A10|A11|A12|A13|A14|A4|A5|A6|A7|A8|A9|B10|B11|B9|BO1|BO2|C4|C5|CO1|CO2|E1|E10|E11|E2|E3|E4|E5|E6|E7|E8|E9|EO1|F1|F10|F11|F14|F20|F21|F23|F24|F25|FO1|G1|G10|G11|G12|G13|G14|G2|G3|G4|G5|G6|G7|G8|G9|GO1|GO2|GO3|GO4|H1|L1|L11|L2|L3|L8|L9|LO1|M1|M10|M3|M4|M7|M9|NA|P1|P10|P11|P13|P3|P4|P5|P6|P7|P8|P9|PO1|PO2|PO3|R1|R10|R11|R13|R2|R2U|R3|R4|R4Y|RO1|S10|S11|S14|S15|S16|S17|S18|S19|S20|S21|S22|S7|S9|SO1|SO2|W1|W10|W11|W13|W14|W15|W2|W3|W4|W5|W6|W8|WO1|WO2|WO3|E14)$/"
        }
      },
      {
        "name": "FMA",
        "type": "string"
      },
      {
        "name": "Township",
        "type": "number",
        "constraints": {
          "min": 1,
          "max": 126
        }
      },
      {
        "name": "Range",
        "type": "string",
        "constraints": {
          "min": 1,
          "max": 26,
          "colmap": true
        }
      },
      {
        "name": "Meridian",
        "type": "number",
        "constraints": {
          "min": 4,
          "max": 6
        }
      },
      {
        "name": "Section",
        "type": "number",
        "constraints": {
          "min": 1,
          "max": 36
        }
      },
      {
        "name": "OpeningNumber",
        "type": "string"
      },
      {
        "name": "SamplingUnitNumber",
        "type": "string"
      },
      {
        "name": "TopographicPosition",
        "type": "string",
        "constraints": {
          "pattern": "/^(1|2|3|4|5|6|7)$/"
        }
      },
      {
        "name": "Elevation",
        "type": "number",
        "constraints": {
          "min": 0,
          "max": 3747
        }
      },
      {
        "name": "Slope",
        "type": "number",
        "constraints": {
          "min": 0,
          "max": 100
        }
      },
      {
        "name": "Aspect",
        "type": "string",
        "constraints": {
          "pattern": "/^(N|E|S|W|NE|SE|SW|NW|NA)$/"
        }
      },
      {
        "name": "XCoord",
        "type": "number",
        "constraints": {
          "min": -111700,
          "max": -1030400
        }
      },
      {
        "name": "YCoord",
        "type": "number",
        "constraints": {
          "min": 5643600,
          "max": 6702500
        }
      },
      {
        "name": "UTMZone",
        "type": "string",
        "constraints": {
          "pattern": "/^(UTM11|UTM12)$/"
        }
      },
      {
        "name": "Datum",
        "type": "string"
      },
      {
        "name": "Latitude",
        "type": "string"
      },
      {
        "name": "Longitude",
        "type": "string"
      },
      {
        "name": "NaturalSubregion",
        "type": "string"
      },
      {
        "name": "ProvincialAreaCode",
        "type": "string",
        "constraints": {
          "pattern": "/^(N|WC|SW)$/"
        }
      },
      {
        "name": "Ecosite",
        "type": "string",
        "constraints": {
          "pattern": "/^(a|b|c|d|e|f|g|h|I|j|k|l|m|n)$/"
        }
      },
      {
        "name": "EcositePhase",
        "type": "string",
        "constraints": {
          "pattern": "/^(1|b1|b2|b3|b4|c1|d1|d2|d3|e1|e2|e3|f1|f2|f3|g1|h1|i1|i2|j1|j2|k1|k2|k3|l1|h2|j3|a2|a3|g2|h3|b5|c2|c3|c4|c5|d4|e4|f4|f5|f6|g3|i3|l2|l3|m1|m2|m3|n1)$/"
        }
      },
      {
        "name": "PlotComment",
        "type": "string"
      }
    ]
  },
  "primaryKey": ["CompanyPlotNumber", "Company"],
  "foreignKeys": [
    {
      "fields": "Company",
      "reference": {
        "resource": "Admin",
        "fields": "Company"
      }
    }
  ]
}